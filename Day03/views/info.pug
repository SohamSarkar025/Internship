
doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title #{pageTitle}
  body
    h1 #{pageTitle}
    h2 #{info.name}
    p Known Languages:
    ul
      if info.language.length > 0
        each lan in info.language
          li #{lan}
      else
        li Unknown
